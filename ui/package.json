{
  "name": "hod-resource-bookings",
  "version": "0.0.0",
  "description": "Webcomponent hod-resource-bookings following open-wc recommendations",
  "author": "hod-resource-bookings",
  "license": "MIT",
  "main": "index.js",
  "module": "index.js",
  "scripts": {
    "storybook": "concurrently --kill-others --names tsc,storybook \"npm run tsc:watch\" \"start-storybook --node-resolve --watch --open\"",
    "storybook:build": "build-storybook",
    "build": "rimraf dist && rollup -c rollup.config.js",
    "start": "concurrently \"rollup -c rollup.config.js -w\" \"es-dev-server --root-dir dist --app-index index.html --open --compatibility none\""
  },
  "dependencies": {
    "@apollo/client": "^3.2.0",
    "@graphql-tools/schema": "^6.2.3",
    "@holochain/conductor-api": "guillemcordoba/holochain-conductor-api#develop",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "buffer": "^5.6.0",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-visualizer": "^4.1.1"
  },
  "devDependencies": {
    "@open-wc/building-rollup": "^1.0.0",
    "@open-wc/demoing-storybook": "^2.0.0",
    "@open-wc/eslint-config": "^2.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/graphql": "^14.5.0",
    "@types/node": "13.11.1",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "concurrently": "^5.1.0",
    "es-dev-server": "^1.23.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.11.0",
    "graphql": "^15.3.0",
    "husky": "^1.0.0",
    "lint-staged": "^10.0.0",
    "prettier": "^2.0.4",
    "rimraf": "^2.6.3",
    "rollup": "^2.3.4",
    "rollup-plugin-node-builtins": "^2.1.2",
    "tslib": "^1.11.0",
    "typescript": "~3.9.0"
  },
  "eslintConfig": {
    "extends": [
      "@open-wc/eslint-config",
      "eslint-config-prettier"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  }
}
